{
  "manifest_version": 2,

  "name": "Netaccess Authenticator",
  "description": "This extension will be used to authenticate netaccess in IITM.",
  "version": "1.0",
  "content_scripts": [
      {
        "matches" : ["https://*/* "],
        "css": ["custom.css"],
        "js" : ["alert.js","test.js"]
      }
    ],

  "permissions": [
    "https://netaccess.iitm.ac.in/account/login"
    ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
